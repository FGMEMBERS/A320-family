<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList include="A320-main.xml">

	<sim>
		<description>Airbus A320-200 with IAE V2500 engines</description>
		<flight-model>jsb</flight-model>
		<aero>A320-231</aero>
		<variant>231</variant>
		<engine>IAE V2500</engine>

		<model>
			<path>Aircraft/A320-family/XMLs/A320-231.xml</path>
			<livery type="string">NWA</livery>
		</model>

		<!-- Flaps only go up to 35 degrees for the 200 -->
		<flaps>
			<setting>0.00</setting>
			<setting>0.25</setting>
			<setting>0.375</setting>
			<setting>0.5</setting>
			<setting>0.875</setting>
		</flaps>

		<menubar>
			<default>
				<menu n="100">
					<label>Airbus A320-231</label>
				</menu>
			</default>
		</menubar>
	</sim>
	
</PropertyList>
