<?xml version="1.0"?>

<PropertyList>

	<path>../Models/a320.hstab.3ds</path> 
	<!--texture-path>../Textures/Livery/House</texture-path-->
	
	<!-- Animation for the elevators. -->
	<animation>
		<name>Elevator.Left</name>
		<type>rotate</type>
		<object-name>ELEVATOR.L</object-name>
		<object-name>HINGE.L</object-name>
		<property>/controls/flight/elevator</property>
		<interpolation>
			<entry><ind>-1.0</ind><dep>-30.0</dep></entry>
			<entry><ind>0.0</ind><dep>0.0</dep></entry>
			<entry><ind>1.0</ind><dep>15.0</dep></entry>
		</interpolation>
		<axis>
			
			<x1-m>4.8217428</x1-m>
			<y1-m>-6.225</y1-m>
			<z1-m>1.2998017</z1-m>
			
			<x2-m>2.9796817</x2-m>
			<y2-m>-0.96224</y2-m>
			<z2-m>0.7486916</z2-m>
			
		</axis>
	</animation>
	
	<animation>
		<name>Elevator.Right</name>
		<type>rotate</type>
		<object-name>ELEVATOR.R</object-name>
		<object-name>HINGE.R</object-name>
		<property>/controls/flight/elevator</property>
		<interpolation>
			<entry><ind>-1.0</ind><dep>-30.0</dep></entry>
			<entry><ind>0.0</ind><dep>0.0</dep></entry>
			<entry><ind>1.0</ind><dep>15.0</dep></entry>
		</interpolation>
		<axis>
			
			<x1-m>2.9796817</x1-m>
			<y1-m>0.96224</y1-m>
			<z1-m>0.7486916</z1-m>
			
			<x2-m>4.8217428</x2-m>
			<y2-m>6.225</y2-m>
			<z2-m>1.2998017</z2-m>
			
		</axis>
	</animation>
	
	<!-- Animation for the horizontal stabilizer. -->
	<animation>
		<name>Horizontal Stabilizer</name>
		<type>rotate</type>
		<property>/controls/flight/elevator-trim</property>
		<interpolation>
			<entry><ind>-1.0</ind><dep>-4.0</dep></entry>
			<entry><ind>1.0</ind><dep>14.0</dep></entry>
		</interpolation>
		<center>
			<x-m>2.4657749</x-m>
			<z-m>0.7464073</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
</PropertyList>