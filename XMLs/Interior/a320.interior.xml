<?xml version="1.0"?>

<PropertyList>

	<path>../../Models/Interior/a320.interior.3ds</path> 
	<!--texture-path>../Textures/Livery/House</texture-path-->
	
	<!-- Turn off shadow for the cabin linings. -->
	<animation>
		<name>Cabin</name>
		<type>noshadow</type>
		<object-name>CABIN</object-name>
	</animation>
	
	<!-- Animation for the doors of the main gears. -->
	<animation>
		<name>Main_Gear_Doors.Left</name>
		<type>rotate</type>
		<object-name>MG DOOR.L</object-name>
		<property>/gear/gear[0]/position-norm</property>
		<interpolation>
			<entry><ind>0.0</ind><dep>0.0</dep></entry>
			<entry><ind>0.2</ind><dep>84.0</dep></entry>
			<entry><ind>0.8</ind><dep>84.0</dep></entry>
			<entry><ind>1.0</ind><dep>0.0</dep></entry>
		</interpolation>
		<center>
			<y-m>-0.4646725</y-m>
			<z-m>-2.3824937</z-m>
		</center>
		<axis>
			<x>1</x>
		</axis>
	</animation>
	
	<animation>
		<name>Main_Gear_Doors.Right</name>
		<type>rotate</type>
		<object-name>MG DOOR.R</object-name>
		<property>/gear/gear[0]/position-norm</property>
		<interpolation>
			<entry><ind>0.0</ind><dep>0.0</dep></entry>
			<entry><ind>0.2</ind><dep>84.0</dep></entry>
			<entry><ind>0.8</ind><dep>84.0</dep></entry>
			<entry><ind>1.0</ind><dep>0.0</dep></entry>
		</interpolation>
		<center>
			<y-m>0.4646725</y-m>
			<z-m>-2.3824937</z-m>
		</center>
		<axis>
			<x>-1</x>
		</axis>
	</animation>
	
