			<entry><ind>0.8</ind><dep>90.0</dep></entry>
			<entry><ind>1.0</ind><dep>0.0</dep></entry>
		</interpolation>
		<axis>
			
			<x1-m>7.599</x1-m>
			<y1-m>-0.5</y1-m>
			<z1-m>-2.0649564</z1-m>
			
			<x2-m>5.394</x2-m>
