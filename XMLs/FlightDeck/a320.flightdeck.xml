	<!-- ANIMATIONS -->
	<!-- Dome lighting -->
	<animation>
		<name>Dome illumination</name>
		<type>material</type>
		<object-name>Cockpit</object-name>
		<object-name>MCP</object-name>
<!--		<object-name>Pilot CDU</object-name>
		<object-name>Copilot CDU</object-name>-->
		<object-name>Pilot radio panel</object-name>
		<object-name>Copilot radio panel</object-name>
		<emission>
			<red>0.5</red>
			<green>0.5</green>
			<blue>0.5</blue>
			<factor-prop>sim/multiplay/generic/float[0]</factor-prop>
		</emission>
	</animation>

	<!-- Panel lighting -->
	<animation>
		<name>Panel lighting select</name>
		<type>select</type>
		<object-name>PanelMarkingLts</object-name>
		<condition>
			<or>
				<not>
					<property>sim/multiplay/generic/float[0]</property>
				</not>
				<and>
					<equals>
						<property>sim/multiplay/generic/float[0]</property>
						<value>0.5</value>
					</equals>
					<greater-than>
						<property>controls/lighting/main-panel-pedestal-norm</property>
						<value>0.25</value>
					</greater-than>
				</and>
				<and>
					<equals>
						<property>sim/multiplay/generic/float[0]</property>
						<value>1</value>
					</equals>
					<greater-than>
						<property>controls/lighting/main-panel-pedestal-norm</property>
						<value>0.5</value>
					</greater-than>
				</and>
			</or>
		</condition>
	</animation>
	<animation>
		<name>Panel no lighting select</name>
		<type>select</type>
		<object-name>PanelMarkings</object-name>
		<condition>
			<or>
				<and>
					<equals>
						<property>sim/multiplay/generic/float[0]</property>
						<value>0.5</value>
					</equals>
					<less-than-equals>
						<property>controls/lighting/main-panel-pedestal-norm</property>
						<value>0.25</value>
					</less-than-equals>
				</and>
				<and>
					<equals>
						<property>sim/multiplay/generic/float[0]</property>
						<value>1</value>
					</equals>
					<less-than-equals>
						<property>controls/lighting/main-panel-pedestal-norm</property>
						<value>0.5</value>
					</less-than-equals>
				</and>
			</or>
		</condition>
	</animation>
	<animation>
		<name>Panel lighting</name>
		<type>material</type>
		<object-name>PanelMarkingLts</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>controls/lighting/main-panel-pedestal-norm</factor-prop>
		</emission>
	</animation>

